{
  "name": "ember-arcgis-portal-services",
  "version": "2.7.9",
  "description": "A set of promise-based Ember Services for working with the ArcGIS Portal API.",
  "keywords": [
    "ember-addon"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/ember-arcgis-portal-services.git"
  },
  "license": "Apache-2.0",
  "author": "Dave Bouwman <dbouwman@esri.com>",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm run lint:js && ember test",
    "test:all": "ember try:each",
    "start:portal": "ember s --env=koopernetes",
    "deploy:devext": "ember surge --new-domain='eapsdev.surge.sh' --environment=devext",
    "deploy:qaext": "ember surge --new-domain='eapsqaext.surge.sh' --environment=qaext",
    "deploy:prod": "ember surge --new-domain='eapsprod.surge.sh' --environment=production",
    "deploy:portaldev": "ember surge --new-domain='eapsportaldev.surge.sh' --environment=portaldev",
    "deploy:portalqa": "ember surge --new-domain='eapsportalqa.surge.sh' --environment=portalqa",
    "deploy:n:prod": "ember b -e production && netlify deploy --prod --dir=dist",
    "deploy:n:qaext": "ember b -e qaext && netlify deploy --alias=qaext --dir=dist",
    "deploy:n:devext": "ember b -e qaext && netlify deploy --alias=devext --dir=dist"
  },
  "dependencies": {
    "ember-auto-import": "^1.5.3",
    "ember-cli-babel": "^6.16.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-fetch": "^6.2.0"
  },
  "peerDependencies": {
    "@esri/arcgis-rest-auth": "^2.4.0",
    "@esri/arcgis-rest-portal": "^2.4.0",
    "@esri/arcgis-rest-request": "^2.4.0",
    "@esri/arcgis-rest-types": "^2.4.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.6.3",
    "@esri/arcgis-rest-auth": "^2.4.0",
    "@esri/arcgis-rest-portal": "^2.19.2",
    "@esri/arcgis-rest-request": "^2.4.0",
    "@esri/arcgis-rest-types": "^2.4.0",
    "broccoli-asset-rev": "^2.7.0",
    "ember-cli": "~3.5.0",
    "ember-cli-active-link-wrapper": "0.5.0",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-eslint": "^4.2.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.3",
    "ember-cli-inject-live-reload": "^1.8.2",
    "ember-cli-jsoneditor": "dbouwman/ember-cli-jsoneditor",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-surge": "1.3.0",
    "ember-cli-uglify": "^2.1.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^3.4.1",
    "ember-resolver": "^5.0.1",
    "ember-sinon": "2.2.0",
    "ember-sinon-qunit": "3.3.0",
    "ember-source": "~3.5.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-truth-helpers": "^2.1.0",
    "ember-try": "^1.2.1",
    "eslint-config-semistandard": "~7.0.0",
    "eslint-config-standard": "~6.0.0",
    "eslint-plugin-ember": "^5.2.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.2.0",
    "eslint-plugin-standard": "^2.0.0",
    "husky": "^1.0.0-rc.13",
    "jsoneditor": "9.5.6",
    "loader.js": "^4.7.0",
    "netlify-cli": "^2.58.0",
    "qunit-dom": "^0.8.0",
    "torii-provider-arcgis": "^3.2.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "contributors": [
    "Mike Juniper <mjuniper@esri.com>",
    "Tom Wayson <twayson@esri.com>"
  ],
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "main": "index.js",
  "bugs": {
    "url": "https://github.com/Esri/ember-arcgis-portal-services/issues"
  },
  "homepage": "https://github.com/Esri/ember-arcgis-portal-services#readme",
  "huskyOut": {
    "hooks": {
      "pre-commit": "npm run lint:js"
    }
  }
}
